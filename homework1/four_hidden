//https://atcoder.jp/contests/abc403/tasks/abc403_b

#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main(){
    
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    
    string t; cin >> t;
    string u; cin >> u;

    bool achou = false;

    for (int i = 0; i < t.size(); i++){

        if (t[i] == u[0] or t[i] == '?'){

            if (u.size() == 1){

                achou = true;

            }

            for (int j = 1; j < u.size(); j++){

                if (j == u.size()-1 and (t[i+j] == u[j] or t[i+j] == '?')){

                    achou = true;

                }

                if (t[i+j] != u[j] and t[i+j] != '?'){

                    break;

                }

            }

        }

    }

    if (achou){

        cout << "Yes" << '\n';

    }

    else if (!achou){

        cout << "No" << '\n';

    }
    

    return 0;
}
