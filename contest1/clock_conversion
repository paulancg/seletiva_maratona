//https://codeforces.com/problemset/problem/1950/C
#include <bits/stdc++.h>
using namespace std;


int main()
{
    int t;
    cin >> t;
    
    for (int i = 0; i < t; i++){
        
        string s;
        cin >> s;
        
        
        if ((s[0] == '1' and s[1] != '0' and s[1] != '1' and s[1] != '2') or s[0] == '2'){
            
            string a = "";
            a = a + s[0] + s[1];
            int ba = stoi(a) - 12;
            if (ba < 10){
                
                string c = to_string(ba);
                s[0] = '0';
                s[1] = c[0];
                
            }
            else{
                
                string b = to_string(ba);
                s[0] = b[0];
                s[1] = b[1];
            }
            
            s = s + " PM";
            
            cout << s << '\n';
            
        }
        
        else if (s[0] == '0' and s[1] == '0'){
            
            s[0] = '1';
            s[1] = '2';
            s = s + " AM";
            
            cout << s << '\n';
        }
        
        else if (s[0] == '1' and s[1] == '2'){
            
            s = s + " PM";
            
            cout << s << '\n';
        }
        
        
        else{
            
            s = s + " AM";
            cout << s << '\n';
            
        }
        
    }

    return 0;
}
