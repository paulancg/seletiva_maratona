//link

#include <bits/stdc++.h>
#define ll long long int
using namespace std;

int main(){
    
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    
    int w; cin >> w;
    int k; cin >> k;
    
    
    int a;
    //2* (a - k) = (a - w);
    //2 * a - 2 *k = a - w;
    a = 2 * k - w;
    
    cout << a << '\n';

    return 0;
}
